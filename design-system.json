      },
      "borderRadius": "borderRadius.small",
      "gap": "spacing.2"
    },
    "button": {
      "primary": {
        "background": "linear-gradient(90deg, var(--colour-city-electric-blue-500) 50%, rgb(26, 180, 246) 100%)",
        "color": "content.onLight",
        "height": "button.height.small",
        "borderRadius": "borderRadius.medium"
      },
      "secondary": {
        "background": "transparent",
        "border": "1px solid",
        "borderColor": "border.action.default",
        "color": "content.primary",
        "height": "button.height.small",
        "borderRadius": "borderRadius.medium"
      }
    },
    "input": {
      "height": "button.height.small",
      "borderRadius": "borderRadius.medium",
      "borderColor": "border.default",
      "backgroundColor": "background.default",
      "padding": {
        "left": "spacing.4",
        "right": "spacing.4",
        "top": "spacing.3",
        "bottom": "spacing.3"
      }
    },
    "card": {
      "borderRadius": "borderRadius.xlarge",
      "border": "1px solid",
      "borderColor": "border.default",
      "padding": "spacing.6",
      "gap": "spacing.6"
    },
    "widget": {
      "backgroundColor": "background.subtlePrimary",
      "borderRadius": "borderRadius.large",
      "border": "1px solid",
      "borderColor": "border.subtle",
